{
  "name": "ellinprincess-landing",
  "version": "1.0.0",
  "description": "Landing page for EllinPrincess services",
  "scripts": {
    "start": "concurrently \"npm run start-react\" \"npm run start-bot\"",
    "start-react": "react-scripts start",
    "start-bot": "cd telegram-bot-server && npm start",
    "install-all": "npm install && cd telegram-bot-server && npm install",
    "build": "react-scripts build",
    "postinstall": "npm run install-all",
    "predeploy": "npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@mui/x-date-pickers": "^6.20.2",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-tsparticles": "^2.12.2",
    "swiper": "^11.2.6",
    "tsparticles": "^3.8.1",
    "tsparticles-slim": "^2.12.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
